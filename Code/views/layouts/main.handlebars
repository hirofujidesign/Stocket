                        <br>
                        <form action="/1" method="post" class="form-horizontal">
                            <div class="form-group">
                                <label for="StockName">Company</label>
                                <input type="text" class="form-control" id="inputCompany" placeholder="Enter Company Name" name="searchTerm" onkeyup="runAuto()"><br>
                                <div id="hintOne"></div><br>
                                <div id="hintTwo"></div><br>
                                <label for="first">Start Date <span class="glyphicon glyphicon-calendar"></span></label><br>
                                <input id="first" type="date" name="beginDate" value="" class="form-control" /><br>
                                <label for="end" >End Date <span class="glyphicon glyphicon-calendar"></span></label><br>
                                <input id="end" type="date" name="endDate" value="" class="form-control" />
                            </div>
                            <br>
