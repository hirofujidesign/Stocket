                        {{#unless logged_in}}<br>
                        <a href="/sign-in"><button class="btn btn-success" id="signing">Sign In</button></a>
                        <a href="/sign-up"><button class="btn btn-success" id="signing">Sign Up</button></a> {{/unless}} {{#if logged_in}} {{#if username}}
